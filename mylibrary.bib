@article{KehleScatteringTheoryLinear2019,
  title = {A {{Scattering Theory}} for {{Linear Waves}} on the {{Interior}} of {{Reissner}}\textendash{{Nordstr\"om Black Holes}}},
  author = {Kehle, Christoph and {Shlapentokh-Rothman}, Yakov},
  year = {2019},
  month = may,
  volume = {20},
  pages = {1583--1650},
  issn = {1424-0661},
  doi = {10.1007/s00023-019-00760-z},
  abstract = {We develop a scattering theory for the linear wave equation \$\$\textbackslash Box \_g \textbackslash psi = 0 \$\$on the interior of Reissner\textendash Nordstr\"om black holes, connecting the fixed frequency picture to the physical space picture. Our main result gives the existence, uniqueness and asymptotic completeness of finite energy scattering states. The past and future scattering states are represented as suitable traces of the solution \$\$\textbackslash psi \$\$on the bifurcate event and Cauchy horizons. The heart of the proof is to show that after separation of variables one has uniform boundedness of the reflection and transmission coefficients of the resulting radial o.d.e. over all frequencies \$\$\textbackslash omega \$\$and~\$\$\textbackslash ell \$\$. This is non-trivial because the natural T conservation law is sign-indefinite in the black hole interior. In the physical space picture, our results imply that the Cauchy evolution from the event horizon to the Cauchy horizon is a Hilbert space isomorphism, where the past (resp. future) Hilbert space is defined by the finiteness of the degenerate T energy fluxes on both components of the event (resp. Cauchy) horizon. Finally, we prove that, in contrast to the above, for a generic set of cosmological constants \$\$\textbackslash Lambda \$\$, there is no analogous finite T energy scattering theory for either the linear wave equation or the Klein\textendash Gordon equation with conformal mass on the (anti-) de~Sitter\textendash Reissner\textendash Nordstr\"om interior.},
  journal = {Annales Henri Poincar\'e},
  language = {en},
  number = {5}
}

@book{KekoMoimenteur2021,
