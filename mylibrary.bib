@article{ThongnakEffectsdapagliflozinstatins2020,
  title = {Effects of Dapagliflozin and Statins Attenuate Renal Injury and Liver Steatosis in High-Fat/High-Fructose Diet-Induced Insulin Resistant Rats},
  author = {Thongnak, Laongdao and Chatsudthipong, Varanuj and Kongkaew, Aphisek and Lungkaphin, Anusorn},
  year = {2020},
  month = jun,
  volume = {396},
  pages = {114997},
  publisher = {{Academic Press Inc Elsevier Science}},
  address = {{San Diego}},
  issn = {0041-008X},
  doi = {10.1016/j.taap.2020.114997},
  abstract = {High-fat high-fructose diet (HFF) in obesity can induce dyslipidemia and lipid accumulation both in kidney and liver which related to insulin resistance and lipotoxicity-induced cellular damage. We investigated whether dapagliflozin with or without atorvastatin could improve lipid accumulation-induced kidney and liver injury in HFF-induced insulin resistant rats. Male Wistar rats were fed with HFF for 16 weeks and then received drug treatments for 4 weeks; vehicle, dapagliflozin, atorvastatin and dapagliflozin plus atorvastatin treatment groups. HFF rats demonstrated insulin resistance, dyslipidemia, liver injury and renal dysfunction associated with impaired renal lipid metabolism and lipid accumulation. Dapagliflozin and combination treatment could improve HFF-induced insulin resistance, lipogenesis and lipotoxicity-related renal oxidative stress, inflammation, fibrosis and apoptosis leading to kidney dysfunction recovery. Liver injury-associated inflammation was also improved by these two regimens. Notably, the reduced lipid accumulation in liver and kidney that linked to an improvement of lipid oxidation was prominent in the combination treatment. Therefore, dapagliflozin combined with atorvastatin treatment exert the beneficial effects on lipid metabolism and lipotoxicity in liver and kidney injury via the attenuation of oxidative stress, fibrosis and apoptosis in insulin resistant model.},
  annotation = {WOS:000529799700005},
  journal = {Toxicology and Applied Pharmacology},
  keywords = {adipose,atorvastatin,Dapagliflozin,inhibition,Insulin Resistance,Liver   Steatosis,obesity,oxidative stress,Renal Lipotoxicity,Statins,steatohepatitis},
  language = {English}
}

