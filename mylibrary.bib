  booktitle = {Les Sciences En Biblioth\`eque},
  author = {Blanchard, Antoine},
  editor = {Netzer, Michel},
  year = {2017},
  month = mar,
  pages = {265},
  publisher = {{\'Editions du cercle de la librairie}},
  abstract = {La r\'evolution du web 2.0, qui vit internet passer d'une dynamique de diffusion \`a une dynamique d'interaction o\`u chaque internaute peut exister et \^etre reconnu par les conversations auxquelles il participe sur diverses plateformes et r\'eseaux sociaux num\'eriques, n'a pas \'epargn\'e la communaut\'e scientifique. En particulier, le format \guillemotleft{} blog \guillemotright{} (terme form\'e par aph\'er\`ese de l'expression \guillemotleft{} web log \guillemotright, pour journal en ligne), initialement utilis\'e pour des journaux intimes, s'est d\'evelopp\'e comme un outil de publication et de conversation r\'epondant aux besoins vari\'es de cette communaut\'e. D'abord isol\'es, puis regroup\'es en communaut\'es, les blogueurs de science (expression qui inclut les scientifiques professionnels mais aussi les \'etudiants, journalistes scientifiques, amateurs de science, mus\'ees et centres de science, groupes concern\'es\ldots{} qui tiennent un blog \`a teneur scientifique) ont d\'emontr\'e leur capacit\'e \`a influencer la mani\`ere dont la recherche avance, dont les r\'esultats sont communiqu\'es et dont les publics sont touch\'es. Parmi les nombreux sujets dont les blogs discutent, je me concentrerai ici sur les sujets proprement scientifiques ou li\'es au monde de la recherche, d\'elimitant ainsi une sous-cat\'egorie du \guillemotleft{} blogging scientifique \guillemotright{} qui fait du num\'erique acad\'emique (\guillemotleft{} digital scholarship \guillemotright ) une conversation (\guillemotleft{} conversational scholarship \guillemotright ). En quoi cela consiste-t-il concr\`etement ? O\`u cette \'evolution nous entraine-t-elle ? Je tenterai de faire la lumi\`ere sur ce ph\'enom\`ene dans ce chapitre.},
  file = {D\:\\Home\\dbelveze\\OneDrive - Université de Rennes 1\\Zotero\\Éditions du cercle de la librairie2017\\Blanchard_2017_Les blogs de science dans la recherche et la médiation scientifique _.pdf},
  keywords = {Blogs de science,Digital scholarship,Médiation scientifique},
  series = {Collection {{Biblioth\`eques}}}
}

