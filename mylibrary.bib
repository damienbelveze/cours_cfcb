  issn = {1424-0661},
  doi = {10.1007/s00023-011-0138-8},
  abstract = {We study the scattering theory for charged Klein\textendash Gordon equations:\$\$\textbackslash left\textbackslash\{\textbackslash begin\{array\}\{lll\}\{( \textbackslash partial\_\{t\}- \{\textbackslash rm i\} v(x))\^\{2\}\textbackslash phi(t,x)+\textbackslash epsilon\^\{2\}(x, D\_\{x\})\textbackslash phi(t,x)=0,\}\textbackslash\textbackslash{} \{\textbackslash phi(0, x)= f\_\{0\},\}\textbackslash\textbackslash{} \{\{\textbackslash rm i\}\^\{-1\} \textbackslash partial\_\{t\}\textbackslash phi(0, x)= f\_\{1\},\}\textbackslash end\{array\}\textbackslash right.\$\$where\$\$\textbackslash begin\{array\}\{lll\}\textbackslash epsilon\^\{2\}(x, D\_\{x\})= -\textbackslash sum \textbackslash limits\_\{1\textbackslash leq j, k\textbackslash leq n\} \textbackslash left( \textbackslash partial\_\{x\_\{j\}\}-\{\textbackslash rm i\} b\_\{j\}(x)\textbackslash right)a\^\{jk\}(x)\textbackslash left( \textbackslash partial\_\{x\_\{k\}\}-\{\textbackslash rm i\} b\_\{k\}(x)\textbackslash right)+ m\^\{2\}(x),\textbackslash end\{array\}\$\$describing a Klein\textendash Gordon field minimally coupled to an external electromagnetic field described by the electric potential v(x) and magnetic potential \$\$\{\textbackslash vec\{b\}(x)\}\$\$. The flow of the Klein\textendash Gordon equation preserves the energy:\$\$\textbackslash begin\{array\}\{lll\}h[f, f]:= \textbackslash int \textbackslash limits\_\{\textbackslash mathbb\{R\}\^\{n\}\} \textbackslash overline\{f\}\_\{1\}(x) f\_\{1\}(x)+ \textbackslash overline\{f\}\_\{0\}(x)\textbackslash epsilon\^\{2\}(x, D\_\{x\})f\_\{0\}(x) - \textbackslash overline\{f\}\_\{0\}(x) v\^\{2\}(x) f\_\{0\}(x) \{\textbackslash rm d\}x. \textbackslash end\{array\}\$\$We consider the situation when the energy is not positive. In this case the flow cannot be written as a unitary group on a Hilbert space, and the Klein\textendash Gordon equation may have complex eigenfrequencies. Using the theory of definitizable operators on Krein spaces and time-dependent methods, we prove the existence and completeness of wave operators, both in the short- and long-range cases. The range of the wave operators are characterized in terms of the spectral theory of the generator, as in the usual Hilbert space case.\vphantom\}},
  journal = {Annales Henri Poincar\'e},
  language = {en},
  number = {4}
}

@article{GillettConsiderationsSafeUse2011,
  title = {Considerations for {{Safe Use}} of {{Statins}}: {{Liver Enzyme Abnormalities}} and {{Muscle Toxicity}}},
