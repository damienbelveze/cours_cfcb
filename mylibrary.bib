  file = {C\:\\Users\\dbelveze\\Dropbox\\fichiers_zotero\\2020_Câmara_Geraldi & al._.pdf;D\:\\Home\\dbelveze\\OneDrive - Université de Rennes 1\\Zotero\\storage\\IEKCQB5I\\S1756464619306772.html},
  journal = {Journal of Functional Foods},
  keywords = {Chia mucilage,Fat reduction,Healthier sausage,In vitro digestion,L,Satiety},
  language = {en}
}

