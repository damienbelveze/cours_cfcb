@misc{Seuratnouveauxsoutienscause2021,
  title = {{Les nouveaux soutiens de la cause palestinienne}},
  author = {Seurat, Le{\"i}la},
  year = {2021},
  month = may,
  abstract = {Apr\`es le cessez-le-feu du 20 mai, la guerre des images continue entre Isra\'eliens et Palestiniens. Ce prolongement m\'ediatique est habituel, mais la cause palestinienne semble s'\^etre r\'eg\'en\'er\'ee \`a l'occasion de ce conflit, mobilisant de nouveaux soutiens au-del\`a de la sph\`ere des militants traditionnels. Les r\'eseaux sociaux qui g\'en\`erent chaque jour une nouvelle vid\'eo virale y participent. Mais ce renouveau profite aussi, en France notamment, de l'affirmation du discours d\'ecolonial, et des mobilisations contre "l'autoritarisme s\'ecuritaire" des politiques s\'ecuritaires.},
